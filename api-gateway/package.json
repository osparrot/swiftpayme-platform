{
  "name": "swiftpayme-api-gateway",
  "version": "1.0.0",
  "description": "SwiftPayMe API Gateway - Unified entry point for all microservices",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "docker:build": "docker build -t swiftpayme-api-gateway .",
    "docker:run": "docker run -p 3000:3000 swiftpayme-api-gateway"
  },
  "keywords": [
    "swiftpayme",
    "api-gateway",
    "microservices",
    "proxy",
    "routing",
    "authentication",
    "rate-limiting",
    "load-balancing",
    "nodejs",
    "typescript",
    "express",
    "fintech"
  ],
  "author": "SwiftPayMe Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "cookie-parser": "^1.4.6",
    "winston": "^3.10.0",
    "express-rate-limit": "^6.10.0",
    "rate-limit-redis": "^4.0.1",
    "redis": "^4.6.7",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "axios": "^1.5.0",
    "async-retry": "^1.3.3",
    "uuid": "^9.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "prom-client": "^14.2.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "yamljs": "^0.3.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "validator": "^13.11.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "express-fileupload": "^1.4.0",
    "node-cache": "^5.1.2",
    "circuit-breaker-js": "^0.0.1",
    "express-slow-down": "^1.6.0",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "express-openapi-validator": "^5.0.6",
    "openapi-types": "^12.1.3",
    "http-status-codes": "^2.2.0",
    "express-async-errors": "^3.1.1",
    "express-promise-router": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/morgan": "^1.9.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/uuid": "^9.0.2",
    "@types/lodash": "^4.14.198",
    "@types/validator": "^13.11.1",
    "@types/multer": "^1.4.7",
    "@types/express-session": "^1.17.7",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/yamljs": "^0.2.31",
    "@types/jest": "^29.5.4",
    "@types/supertest": "^2.0.12",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swiftpayme/api-gateway.git"
  },
  "bugs": {
